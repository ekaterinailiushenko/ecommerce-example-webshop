{
  "name": "ecommerce-example-webshop",
  "private": true,
  "version": "1.3.0",
  "homepage": "https://ekaterinailiushenko.github.io/ecommerce-example-webshop",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postinstall": "sh ./scripts/patch-vitest-fail-on-console.sh && patch-package",
    "lint": "tsc --noEmit && eslint ./ --ext ts --ext tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "preview": "vite preview",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm run lint",
    "*.{ts,tsx}": "npm run prettier"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "classnames": "^2.5.1",
    "firebase": "^10.12.2",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.2.1",
    "react-router": "^7.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.10",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-hooks-addons": "^0.3.1",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unused-imports": "^4.1.3",
    "gh-pages": "^6.1.1",
    "history": "^5.3.0",
    "husky": "^9.1.6",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.10",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.7.3",
    "vite": "^5.1.0",
    "vitest": "^2.1.8",
    "vitest-fail-on-console": "^0.7.1"
  }
}
